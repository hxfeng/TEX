<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<extension>
<id>com.fasterxml.jackson.core:jackson-core</id>
<version>2.8.4</version>
<type>jar</type>
<name>Jackson-core</name>
<summary>Core Jackson abstractions, basic JSON streaming API implementation</summary>
<website>https://github.com/FasterXML/jackson-core</website>
<authors>
<author>
<name>Tatu Saloranta</name>
</author>
<author>
<name>Christopher Currie</name>
</author>
<author>
<name>Paul Brown</name>
</author>
</authors>
<licenses>
<license>
<name>Apache License 2.0</name>
</license>
</licenses>
<scm>
<url>http://github.com/FasterXML/jackson-core</url>
<connection>
<system>git</system>
<path>git@github.com:FasterXML/jackson-core.git</path>
</connection>
<developerconnection>
<system>git</system>
<path>git@github.com:FasterXML/jackson-core.git</path>
</developerconnection>
</scm>
<issuemanagement>
