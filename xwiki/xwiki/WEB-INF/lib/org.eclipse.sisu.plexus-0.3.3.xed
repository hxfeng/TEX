<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<extension>
<id>org.eclipse.sisu:org.eclipse.sisu.plexus</id>
<version>0.3.3</version>
<type>eclipse-plugin</type>
<summary>Plexus-JSR330 adapter; adds Plexus support to the Sisu-Inject container</summary>
<website>http://www.eclipse.org/sisu/org.eclipse.sisu.plexus/</website>
<licenses>
<license>
<name>Eclipse Public License - v 1.0</name>
</license>
</licenses>
<scm>
<url>http://git.eclipse.org/c/sisu/org.eclipse.sisu.plexus.git/tree/org.eclipse.sisu.plexus/</url>
<connection>
<system>git</system>
<path>git://git.eclipse.org/gitroot/sisu/org.eclipse.sisu.plexus.git/org.eclipse.sisu.plexus</path>
</connection>
<developerconnection>
<system>git</system>
<path>ssh://git.eclipse.org/gitroot/sisu/org.eclipse.sisu.plexus.git/org.eclipse.sisu.plexus</path>
</developerconnection>
</scm>
<issuemanagement>
<system>bugzilla</system>
<url>https://bugs.eclipse.org/bugs/enter_bug.cgi?product=Sisu&amp;component=Plexus&amp;format=guided</url>
</issuemanagement>
<dependencies>
